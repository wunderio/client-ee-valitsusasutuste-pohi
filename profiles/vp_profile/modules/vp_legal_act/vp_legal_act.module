<?php
/**
 * @file
 * Code for the VP Legal act feature.
 */

include_once 'vp_legal_act.features.inc';

/**
 * Implements hook_views_query_alter().
 */
function vp_legal_act_views_query_alter(&$view, &$query) {
  if ($view->name == 'legal_acts') {
    drupal_add_js(drupal_get_path('module', 'vp_legal_act') . '/js/bef-tree-select-sub-checkboxes.js',
      array('scope' => 'footer')
    );
  }
}
